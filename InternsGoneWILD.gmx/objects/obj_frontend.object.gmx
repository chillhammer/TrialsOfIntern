            <string>/// Draw Intern Heads
var num = ds_list_size(global.fightersNames);

var b_width = 48;
var pad = 16;

var t_width = (b_width + pad) * (num - 1) + b_width;
var x1 = x - t_width * 0.5;
var y1 = y;

for (i = 0; i &lt; num; i++) {
    var f = global.fighters[? global.fightersNames[|i]];
    draw_set_color(c_white);
    var angle = 0;
    if (point_in_rectangle(mouse_x, mouse_y, x1, y1, x1 + b_width, y1 + b_width)) {
        angle = sin(current_time / 50) * 15 - 7;
        draw_rectangle(x1, y1, x1 + b_width, y1 + b_width, true);
        if (mouse_check_button_pressed(mb_left)) {
            global.fighterPlayerName = global.fightersNames[|i];
            room_goto(rm_fighting);
        }
    }
    
    draw_sprite_ext(f[?"head_spr"], 0, x1 + 24, y1 + 30, 2, 2, angle, c_white, 1);
    
    draw_set_font(fnt_hud);
    draw_set_halign(fa_center);
    draw_text(x1 + 24, y1 + 48, global.fightersNames[|i]);
    
    x1 += b_width + pad;
}

draw_set_font(fnt_hud_big);
draw_text(room_width *0.5, y1 + 64, "Choose your intern");
draw_set_font(fnt_hud);
draw_text(room_width *0.5, y1 + 128, "Made by Andrew Hoyt#Controls: WASD, Mouse, Shift");
</string>
